{
  "name": "insights-service",
  "version": "1.0.0",
  "description": "Insights Microservice - Bubble Technical Test",
  "scripts": {
    "start": "node dist/src/server.js",
    "build": "rm -rf dist/* && tsc && npm run copy-definitions",
    "copy-definitions": "cp -r src/config/swagger.yaml ./dist/src/config",
    "test": "npm run lint && NODE_ENV=test jest --forceExit --detectOpenHandles --collectCoverage --bail",
    "lint": "node_modules/eslint/bin/eslint.js '*/**/*.{js,ts,tsx}' --quiet --fix",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.js",
    "db:sync": "npm run build && npm run typeorm:cli schema:sync",
    "db:reset": "npm run build && npm run typeorm:cli schema:drop && npm run typeorm:cli schema:sync"
  },
  "author": "Nick Davies <nickdavies@gmail.com>",
  "license": "none",
  "dependencies": {
    "axios": "^0.21.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-jwt": "^6.0.0",
    "express-jwt-authz": "^2.4.1",
    "helmet": "^4.1.1",
    "http-status-codes": "^2.1.4",
    "jest": "^26.6.1",
    "jwks-rsa": "^1.11.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.6",
    "pg": "^8.4.2",
    "superagent": "^6.1.0",
    "supertest": "^5.0.0",
    "swagger-express-middleware": "^4.0.2",
    "typeorm": "^0.2.28",
    "typescript-tslint-plugin": "^0.5.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/compression": "^1.7.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.8",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^26.0.15",
    "@types/jest-diff": "^24.3.0",
    "@types/morgan": "^1.9.2",
    "@types/supertest": "^2.0.10",
    "@types/swagger-express-middleware": "^1.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "codecov": "^3.8.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest-express": "^1.12.0",
    "jest-mock-extended": "^1.0.10",
    "nock": "^13.0.4",
    "prettier": "^2.1.2",
    "ts-auto-mock": "^2.6.5",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm test",
      "pre-push": "npm run build && npm test"
    }
  }
}
